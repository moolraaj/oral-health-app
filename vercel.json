{
    "version": 2,
    "builds": [
      { "src": "apps/desktop/package.json", "use": "@vercel/next" },
      { "src": "apps/mobile/package.json", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/desktop/(.*)", "dest": "apps/desktop/$1" },
      { "src": "/mobile/(.*)", "dest": "apps/mobile/$1" }
    ]
  }